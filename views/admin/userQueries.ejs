<%- include('./includes/sidebar.ejs') %>

    <h1 class="p-4">User Queries</h1>

    <% if(queries.length===0) { %>
        <h5 class="m-4 p-4">
            No Queries
        </h5>
        <% } %>
            <!---User profile starts-->

            <div class="card-body tab-content border-0">
                <% queries.forEach(query=>{ %>
                    <!---//Actual Profile Data-->
                    <div class="tab-pane active" id="profile">
                        <hr>

                        <p><b>Name: </b>
                            <%= query.name %>
                        </p>
                        <p><b>Email: </b>
                            <%= query.email %>
                        </p>
                        <p><b>Description: </b>
                            <%= query.message %>
                        </p>


                    </div>
                    <%})%>

            </div>

            </div>
            </div>



            </div>
            </div>
            </div>
            </div>

            <!-- Optional JavaScript; choose one of the two! -->

            <!-- Option 1: Bootstrap Bundle with Popper -->
